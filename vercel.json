{
  "version": 2.0,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "devCommand": "npm run dev",
  "startCommand": "npm run start",
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
